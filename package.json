{
  "name": "lrc-maker",
  "version": "1.0.0",
  "description": "歌词滚动姬｜可能是你所能见到的最好的歌词制作工具",
  "scripts": {
    "start": "npm run webpack && npm run render && webpack-dev-server --hot --open",
    "build": "npm run webpack:prod && npm run render",
    "webpack": "webpack --progress --colors",
    "webpack:prod": "webpack -p --define process.env.NODE_ENV='\"production\"' --progress --colors",
    "render": "static-react src/js/components/Root.js > index.html",
    "server": "webpack-dev-server --hot --open"
  },
  "keywords": [
    "lyric maker",
    "neteasemusic"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hufan-Akari/LRC-MAKER.git"
  },
  "author": "阿卡琳",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/hufan-Akari/LRC-MAKER/issues"
  },
  "homepage": "https://github.com/hufan-Akari/LRC-MAKER#readme",
  "dependencies": {
    "jump.js": "^1.0.2",
    "mousetrap": "^1.6.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.26.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "normalize.css": "^5.0.0",
    "static-react": "^3.2.0",
    "style-loader": "^0.13.2",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  }
}
